{
  "branch": "master",
  "repositoryUrl": "https://github.com/micell/micell.git",
  "tagFormat": "${version}",
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    ["@semantic-release/npm", {
      "pkgRoot": "build",
    }],
    ["@semantic-release/git", {
      "assets": ["build/**", "package.json"]
    }]
  ]
}
