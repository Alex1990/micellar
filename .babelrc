{
  "presets": [
    [
      "@babel/preset-typescript",
      {
        "allowNamespaces": true
      }
    ],
    [
      "@babel/preset-env",
      {
        "modules": false
      }
    ]
  ],
  "plugins": ["@babel/plugin-transform-runtime"],
  "env": {
    "commonjs": {
      "plugins": [
        ["@babel/plugin-transform-modules-commonjs"],
        ["babel-plugin-add-module-exports"]
      ]
    }
  }
}
