{
  "name": "micell",
  "version": "0.15.2",
  "description": "A collection of functions for frontend development",
  "main": "dist/micell.common.js",
  "module": "dist/micell.esm.js",
  "jsdelivr": "dist/micell.js",
  "unpkg": "dist/micell.js",
  "types": "index.d.ts",
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "clean": "rimraf dist lib es types coverage",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "lint": "eslint . --ext .ts",
    "type-check": "tsc --noEmit",
    "type-check:watch": "yarn run type-check -- --watch",
    "prettier": "prettier --write .",
    "vitest": "vitest",
    "test": "vitest",
    "test:ci": "vitest run --browser.headless",
    "test:cover": "vitest --coverage",
    "test:ci:cover": "vitest --coverage --browser.headless",
    "build": "yarn run clean && yarn run types && yarn run build:commonjs && yarn run rollup",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --extensions .mjs,.js,.ts --out-dir lib",
    "rollup": "rollup -c",
    "types": "node scripts/generateDeclaration.js",
    "pkg": "node scripts/release.js",
    "release": "yarn run pkg && semantic-release",
    "postpublish": "node ../scripts/postpublish.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/micell/micell.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run type-check && lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint . --ext .ts --fix",
      "prettier --write"
    ]
  },
  "keywords": [
    "micell",
    "utility",
    "util",
    "base64",
    "cookie",
    "date",
    "dom",
    "easing",
    "emitter",
    "path",
    "qs",
    "querystring",
    "regex",
    "string",
    "ua",
    "useragent",
    "url"
  ],
  "author": "Alex Chao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/micell/micell/issues"
  },
  "homepage": "https://micell.org",
  "devDependencies": {
    "@babel/cli": "^7.16.7",
    "@babel/core": "^7.17.9",
    "@babel/node": "^7.16.7",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.16.7",
    "@babel/plugin-transform-modules-commonjs": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@commitlint/cli": "^16.0.1",
    "@commitlint/config-conventional": "^16.0.0",
    "@rollup/plugin-typescript": "^4.0.0",
    "@semantic-release/git": "^10.0.1",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@types/sinon": "^10.0.6",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "@vitest/browser": "^2.0.5",
    "@vitest/coverage-istanbul": "^2.0.5",
    "@vuepress/plugin-google-analytics": "^1.0.0",
    "@vuepress/plugin-pwa": "^1.0.0",
    "babel-loader": "^8.2.4",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-plugin-istanbul": "^5.2.0",
    "chai": "^4.2.0",
    "chalk": "^2.4.2",
    "cpy-cli": "^3.1.1",
    "cross-env": "^5.2.0",
    "es6-promise": "^4.2.8",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^2.7.0",
    "lint-staged": "^12.1.4",
    "lodash": "^4.17.21",
    "mkdirp": "^1.0.4",
    "mocha": "^6.1.4",
    "playwright": "^1.45.3",
    "prettier": "^2.7.1",
    "replace": "^1.2.1",
    "request": "^2.88.0",
    "rimraf": "^2.6.1",
    "rollup": "^2.2.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^5.3.0",
    "semantic-release": "^18.0.1",
    "shelljs": "^0.8.3",
    "sinon": "^12.0.0",
    "snazzy": "^9.0.0",
    "tslib": "^1.11.1",
    "typescript": "^3.8.3",
    "vite": "^5.3.5",
    "vitest": "^2.0.5",
    "vuepress": "^1.9.5",
    "webpack": "^4.44.1"
  },
  "dependencies": {
    "@babel/runtime": "^7.9.0",
    "@babel/runtime-corejs3": "^7.8.7",
    "core-js": "^3.6.4"
  }
}
