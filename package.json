{
  "name": "micell",
  "version": "0.15.2",
  "description": "A collection of functions for frontend development",
  "main": "dist/micell.common.js",
  "module": "dist/micell.esm.js",
  "jsdelivr": "dist/micell.js",
  "unpkg": "dist/micell.js",
  "types": "index.d.ts",
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "clean": "rimraf dist lib es types coverage",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "predocs:build": "cd docs && yarn",
    "lint": "biome check --files-ignore-unknown=true",
    "type-check": "tsc --noEmit",
    "type-check:watch": "yarn run type-check -- --watch",
    "prettier": "prettier --write .",
    "vitest": "vitest",
    "test": "vitest",
    "test:ci": "vitest run --browser.headless",
    "test:cover": "vitest --coverage",
    "test:ci:cover": "vitest run --coverage --browser.headless",
    "build": "yarn run clean && yarn run types && yarn run build:commonjs && yarn run rollup",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --extensions .mjs,.js,.ts --out-dir lib",
    "rollup": "rollup -c",
    "types": "node scripts/generateDeclaration.js",
    "pkg": "node scripts/release.js",
    "release": "yarn run pkg && semantic-release",
    "postpublish": "node ../scripts/postpublish.js",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/micell/micell.git"
  },
  "lint-staged": {
    "*.ts": [
      "biome check --write"
    ]
  },
  "keywords": [
    "micell",
    "utility",
    "util",
    "base64",
    "cookie",
    "date",
    "dom",
    "easing",
    "emitter",
    "path",
    "qs",
    "querystring",
    "regex",
    "string",
    "ua",
    "useragent",
    "url"
  ],
  "author": "Alex Chao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/micell/micell/issues"
  },
  "homepage": "https://micell.org",
  "devDependencies": {
    "@babel/cli": "^7.24.8",
    "@babel/core": "^7.25.2",
    "@babel/helper-environment-visitor": "^7.24.7",
    "@babel/node": "^7.25.0",
    "@babel/plugin-transform-modules-commonjs": "^7.24.8",
    "@babel/plugin-transform-runtime": "^7.24.7",
    "@babel/preset-env": "^7.25.3",
    "@babel/preset-typescript": "^7.24.7",
    "@biomejs/biome": "1.8.3",
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.3",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.7",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.5",
    "@semantic-release/git": "^10.0.1",
    "@types/shelljs": "^0.8.15",
    "@types/sinon": "^10.0.6",
    "@vitest/browser": "^2.0.5",
    "@vitest/coverage-istanbul": "^2.0.5",
    "babel-plugin-add-module-exports": "^1.0.2",
    "chalk": "^2.4.2",
    "cross-env": "^5.2.0",
    "husky": "^9.1.4",
    "lint-staged": "^12.1.4",
    "lodash": "^4.17.21",
    "playwright": "^1.46.0",
    "replace": "^1.2.2",
    "rimraf": "^5.0.10",
    "rollup": "^4.20.0",
    "semantic-release": "^18.0.1",
    "shelljs": "^0.8.5",
    "sinon": "^18.0.0",
    "tslib": "^2.6.3",
    "typescript": "^5.5.4",
    "vite": "^5.4.0",
    "vitepress": "^1.3.2",
    "vitest": "^2.0.5"
  },
  "dependencies": {
    "@babel/runtime": "^7.25.0",
    "@babel/runtime-corejs3": "^7.25.0",
    "core-js": "^3.38.0"
  }
}
