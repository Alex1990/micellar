{
  "name": "micell",
  "version": "0.2.0",
  "description": "A collection of functions for frontend development",
  "main": "dist/micell.common.js",
  "module": "dist/micell.esm.js",
  "jsdelivr": "dist/micell.js",
  "unpkg": "dist/micell.js",
  "types": "index.d.ts",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "clean": "rimraf dist lib es",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "lint": "standard '**/*.js' --verbose | snazzy",
    "lint:fix": "standard '**/*.js' --fix --verbose | snazzy",
    "test": "cross-env BABEL_ENV=test karma start",
    "test:ci": "npm run test -- --single-run",
    "build": "npm run clean && npm run build:commonjs && npm run rollup",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "rollup": "rollup -c",
    "prepare": "node scripts/release.js",
    "release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/micell/micell.git"
  },
  "standard": {
    "env": [
      "node",
      "browser",
      "mocha"
    ],
    "globals": [
      "chai",
      "sinon",
      "expect"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.js": [
      "standard --fix",
      "git add"
    ]
  },
  "keywords": [
    "micellar",
    "utility",
    "util"
  ],
  "author": "Alex Chao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/micell/micell/issues"
  },
  "homepage": "https://micell.org",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-transform-modules-commonjs": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@semantic-release/git": "^7.0.16",
    "@vuepress/plugin-google-analytics": "next",
    "@vuepress/plugin-pwa": "next",
    "babel-loader": "^8.0.5",
    "babel-plugin-add-module-exports": "^1.0.2",
    "chai": "^4.2.0",
    "chalk": "^2.4.2",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "husky": "^2.1.0",
    "karma": "^4.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-file-fixtures-preprocessor": "^2.0.1",
    "karma-mocha": "^1.3.0",
    "karma-webpack": "^4.0.2",
    "lint-staged": "^8.1.5",
    "lodash": "^4.17.14",
    "mkdirp": "^0.5.1",
    "mocha": "^6.1.4",
    "request": "^2.88.0",
    "rimraf": "^2.6.1",
    "rollup": "^1.17.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^5.1.1",
    "semantic-release": "^15.13.19",
    "shelljs": "^0.8.3",
    "sinon": "^7.3.2",
    "snazzy": "^8.0.0",
    "standard": "^12.0.1",
    "vuepress": "^1.0.2",
    "webpack": "^4.30.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.4.4"
  }
}
